{
  "name": "@griffins-scout/api",
  "version": "1.0.0",
  "description": "The browser API collection for anything Griffins Scout",
  "main": "./src/index.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "wireit",
    "dev": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc --build --pretty",
      "clean": "if-file-deleted",
      "files": [
        "src/**"
      ],
      "output": [
        "dist/**"
      ]
    },
    "dev": {
      "command": "tsc --build --pretty",
      "clean": "if-file-deleted",
      "files": [
        "src/**"
      ],
      "output": [
        "dist/**"
      ]
    }
  },
  "author": "omagarwal25",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@griffins-scout/game": "*"
  }
}
